{
	"presets": [
		[
      "env",
			{
				"modules": false,
				"targets": {
					"browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
				}
      }
		],
    "stage-2",
		"es2015",
		"react"
	],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
	"env": {
		"test": {
      "presets": ["env", "stage-2", "es2015", "react"],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    }
	}
}
